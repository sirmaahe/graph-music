{
  "name": "golang-relay-starter-kit",
  "private": true,
  "description": "A quick way to get up and running with Relay on Golang",
  "repository": "github.com/sogko/golang-relay-starter-kit",
  "version": "0.1.0",
  "scripts": {
    "start": "go run cmd/server/main.go & babel-node ./server.js",
    "update-schema": "go run cmd/update/main.go && relay-compiler --src ./js --schema ./shared/schema.json"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-relay": "^1.6.2",
    "babel-relay-plugin": "^0.11.0",
    "classnames": "^2.1.3",
    "express": "^4.16.4",
    "graphql": "^0.13.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-relay": "^1.6.2",
    "relay-compiler": "^1.6.2",
    "webpack": "^4.20.2",
    "webpack-dev-server": "^3.1.9",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-cli": "^6.26.0"
  }
}
